{
  "name": "jsroot",
  "homepage": "https://root.cern/js/",
  "authors": [
    "Sergey Linev <S.Linev@gsi.de>",
    "Bertrand Bellenot <bertrand.bellenot@cern.ch>"
  ],
  "repository": {
     "type": "git",
     "url": "git://github.com/root-project/jsrootr.git"
  },
  "description": "JavaScript ROOT library",
  "main": "scripts/JSRootCore.js",
  "moduleType": [
    "globals",
    "amd",
    "node"
  ],
  "dependencies": {
    "d3": "~4.4.4",
    "jquery": "3.1.1",
    "jquery-ui": "1.12.1",
    "jquery-mousewheel" : "3.1.13",
    "jqueryui-touch-punch" : "*",
    "MathJax" : "2.6.1",
    "threejs": "*",
    "dat.gui": "*"
  },
  "overrides": {
     "BOWER-PACKAGE": {
        "main": [ 
           "scripts/JSRootCore.js",
           "scripts/rawinflate.min.js", 
           "scripts/JSRootIOEvolution.js",
           "scripts/JSRootMath.js",
           "scripts/JSRootTree.js",
           "scripts/JSRootPainter.js",
           "scripts/JSRootPainter.more.js",
           "scripts/JSRootPainter.jquery.js",
           "scripts/saveSvgAsPng.min.js",
           "scripts/JSRoot3DPainter.js",
           "scripts/three.extra.min.js",
           "scripts/ThreeCSG.js",
           "scripts/JSRootGeoBase.js",
           "scripts/JSRootGeoPainter.js",
           "style/JSRootPainter.css",
           "style/JSRootGeoPainter.css",
           "style/jquery-ui.css"
         ]
      }
  },
  "keywords": [
    "jsroot",
    "ROOT",
    "CERN"
  ],
  "ignore": [
    "*.md",
    "/demo",
    "/docs",
    "/files",
    "/libs",
    "/LICENSE"
   ],
  "license": "MIT"
}
