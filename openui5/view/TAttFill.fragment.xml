    <core:FragmentDefinition
    	xmlns="sap.m"
    	xmlns:l="sap.ui.layout"
    	xmlns:f="sap.ui.layout.form"
    	xmlns:core="sap.ui.core">
    	<VBox class="sapUiSmallMargin">
    		<f:SimpleForm id="TAttFill"
    			editable="true"
    			layout="ResponsiveGridLayout"
    			title="TAttFill"
    			labelSpanXL="1"
    			labelSpanL="2"
    			labelSpanM="4"
    			labelSpanS="6"
    			adjustLabelSpan="false"
    			emptySpanXL="0"
    			emptySpanL="0"
    			emptySpanM="0"
    			emptySpanS="0"
    			columnsXL="1"
    			columnsL="1"
    			columnsM="1"
    			breakpointXL="800"
    			breakpointL="500"
    			breakpointM="300"
    			singleContainerFullSize="false" >
    			<f:content>
    				<Label text="Style" />
    				<Text text="{/fFillStyle}" />
    				<Label text="Color" />
    				<Button text="{/fFillColor}" press="processTAttFill_Color"/>
    			</f:content>
    		</f:SimpleForm>
    	</VBox>
    </core:FragmentDefinition>